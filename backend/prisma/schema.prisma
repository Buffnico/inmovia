generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id        Int      @id @default(autoincrement())
  nombre    String
  apellido  String
  dni       String   @unique
  email     String?
  telefono  String?
  createdAt DateTime @default(now())
}
